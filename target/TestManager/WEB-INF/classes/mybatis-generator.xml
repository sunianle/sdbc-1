<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- 制定mysql的驱动包的路径 千万别放中文路径下 -->
	<classPathEntry location="C:/Users/Administrator/Desktop/mysql-connector-java-5.1.10-bin.jar" />
	<!--D:\DVD/mysql-connector-java-5.0.8-bin.jar  -->
	<!-- 配置数据源和生成的代码所存放的位置 -->
	<context id="context1">

		<commentGenerator>
			<!-- 去除自动生成的注释 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		
		 <jdbcConnection 
			driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/mcdb" 
			userId="root"
			password="password" />
		<!-- 所生成的实体类的位置默认资源包src -->
		<javaModelGenerator targetPackage="com.shudun.mc.pojo"
			targetProject="Mcmanager" />
		<!-- 所生成的sqlMap的影射文件的位置,默认资源包src -->
		<sqlMapGenerator targetPackage="com.shudun.mc.sql.mapper"
			targetProject="Mcmanager" />

		<!-- 为哪些表生成代码 tableName:表名 schema:不用填写,其余属性是禁用例子查询的生成 -->
		<table schema="" tableName="t_device_information" enableCountByExample="false"
		 	enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false" >
		</table>
		
	
		
	</context>
</generatorConfiguration>
